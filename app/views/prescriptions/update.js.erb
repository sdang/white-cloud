<% unless @prescription.valid? %>
	$("#prescription-form-errors").html("<%= escape_javascript(errors_for(@prescription, 'Unable to udpate prescription'))%>")
	$("#prescription-form-errors").effect("highlight", {}, 3000)
<% else %>
	$("#prescription-<%= @prescription.id %>").html("<%= escape_javascript(render @prescription) %>");
	$("#prescription-<%= @prescription.id %>").effect("highlight", {}, 3000);
<% end %>
