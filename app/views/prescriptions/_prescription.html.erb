<div id="prescription-<%= prescription.id -%>" class="reminder">
	<div id="links">
		<%= link_to image_tag("delete_16.gif", :border => 0), {:action => "destroy", :id => prescription.id, :controller => "prescriptions"}, :method => :delete, :remote => true, :confirm => "Delete this Rx?" %><span style="width: 10px">&nbsp;</span>
		<%= link_to image_tag("edit_16.gif", :border => 0), {:action => "edit", :id => prescription.id, :controller => "prescriptions"}, :remote => true %>
	</div>
	<div id="content">
		<%= prescription.in_medlish(session[:group_password]) %>
		<% unless prescription.print? %>
			<br/><span id="grayText">Please add sig and quantity to have this Rx printed</span>
		<% end %>
	</div>
	<div style="clear: both"></div>
</div>