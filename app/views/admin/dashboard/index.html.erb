<div class="tabbable">
	<div class="tab-content">
		<div id="user_list" class="tab-pane active">
			<%= render :partial => "users"%>
		</div>
		<div id="reminder_list" class="tab-pane">
			<%= render :partial => "reminder_lists"%>
		</div>
		<div id="review_logs" class="tab-pane">
			<%= render :partial => "logs"%>
		</div>
	</div>
</div>


<%= content_for :sidebar do %>
	<div id="sideBarWrapper">
		<div id="followContent">
		<ul class="nav nav-pills nav-stacked">
			<li class="active"><%= link_to "Users", "#user_list", :'data-toggle' => "tab" -%></li>
			<li><%= link_to "Reminder Lists", "#reminder_list", :'data-toggle' => "tab" -%></li>
			<li><%= link_to "Review Logs", "#review_logs", :'data-toggle' => "tab" -%></li>
		</ul>
		</div>
	</div>

<% end %>