

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
	<%= f.label :email, "Email Address" %><br/>
	<%= f.email_field :email, :title => "name@domain.com", :autocomplete => "off", :id => "text-field-input" %><br/>
	<%= f.label :password, "Password" %><br/>
	<%= f.password_field :password, :title => "**********", :id => "text-field-input" %><br/>
	<center><%= image_submit_tag "login-button.png", :id => "custom-button" -%></center>
<% end %>
<br/>
<%= render :partial => "devise/shared/links" %>
